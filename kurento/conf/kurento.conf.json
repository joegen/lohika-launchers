{
  "_params" : {
    "executable_prefix": "/usr/bin/",
    "executable" : "kurento-media-server"
  },

  "mediaServer" : {
    "resources": {
      "exceptionLimit": "0.8",
      "killLimit": "0.7",
        "garbageCollectorPeriod": 240
    },
    "net" : {
      "websocket": {
        "port": 8888,
        "registrar": {
         "address": "ws://localhost:9090",
         "localAddress": "localhost"
        },
        "path": "kurento",
        "threads": 10
      }
    }
  }
}
